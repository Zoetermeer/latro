#lang at-exp racket

(require "common.rkt"
         rackunit)

(compile!)

#|
@interp{
  fun +(a, b) = primitive("int+", a, b)
  fun -(a, b) = primitive("int-", a, b)
  fun *(a, b) = primitive("int*", a, b)
  fun /(a, b) = primitive("int/", a, b)

  infix - 2
  infix + 2
  infix / 1
  infix * 1

  1 + 2 * 3 - 4 / 5
}
|#

@alpha-convert{
  fun <=(0, 0) = True
  fun <=(x, y) = (x - 1) <= (y - 1)

  10 <= 10
}
